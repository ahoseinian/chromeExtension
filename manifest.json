{
  "name": "G5line.com RSS Fetcher",
  "version": "0.1",

  "description": "Keep up with the latest from G5line.",
  "icons": { "16" : "images/icon.png",
 			"48" : "images/48.png",
			"128" : "images/128.png"},
  "homepage_url": "http://g5line.com/",
	"background_page": "background.html",
	"permissions": [
	  "http://g5line.com/*",
    "notifications"
	],
	"browser_action": {
    "default_icon": "images/icon.png", 
    "default_title": "G5line Notifier",      
    "default_popup": "popup.html"       
  }
}